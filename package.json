{
	"name": "@stackauth/nestjs",
	"version": "1.0.0",
	"description": "Pacote de consumo de API de autenticação para aplicativos NestJS",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"watch": "tsc --watch",
		"start:dev": "tsc --watch",
		"build:example": "nest build example",
		"start:example": "nest start example --watch",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\"",
		"prepublishOnly": "yarn test && yarn build",
		"preversion": "yarn lint",
		"version": "yarn format && git add -A src",
		"postversion": "git push && git push --tags",
		"test": "jest --config jest.config.js --detectOpenHandles",
		"test:watch": "jest --config jest.config.js --watch",
		"test:cov": "jest --config jest.config.js --coverage",
		"semantic-release": "semantic-release"
	},
	"keywords": [
		"api",
		"client",
		"typescript",
		"nestjs",
		"authentication",
		"auth"
	],
	"author": {
		"name": "StackAuth Team",
		"email": "weltongbi@gmail.com",
		"url": "https://github.com/welton-dev/stackauth"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stackauth/nestjs.git"
	},
	"bugs": {
		"url": "https://github.com/stackauth/nestjs/issues"
	},
	"homepage": "https://github.com/stackauth/nestjs#readme",
	"peerDependencies": {
		"@nestjs/axios": "^3.1.3",
		"@nestjs/common": "^10.0.0",
		"@nestjs/core": "^10.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.0.0"
	},
	"devDependencies": {
		"@apollo/server": "^4.11.3",
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@golevelup/ts-jest": "^0.6.1",
		"@nestjs/apollo": "^12.2.2",
		"@nestjs/axios": "^3.1.3",
		"@nestjs/cli": "^10.4.9",
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.3.0",
		"@nestjs/core": "^10.0.0",
		"@nestjs/devtools-integration": "^0.1.6",
		"@nestjs/graphql": "^12.2.2",
		"@nestjs/platform-express": "^10.4.15",
		"@nestjs/testing": "^10.0.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-watch": "^8.0.0",
		"graphql": "^16.10.0",
		"husky": "^8.0.0",
		"jest": "^29.0.0",
		"jest-mock-extended": "^4.0.0-beta1",
		"kill-port": "^2.0.1",
		"onchange": "^7.1.0",
		"prettier": "^3.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.0.0",
		"semantic-release": "^21.0.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"axios": "^1.6.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"sideEffects": false
}
